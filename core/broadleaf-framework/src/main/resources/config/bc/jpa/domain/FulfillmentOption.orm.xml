<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd" version="2.0">

    <named-query name="BC_READ_ALL_FULFILLMENT_OPTIONS" >
        <query>SELECT fulfillmentOption FROM org.broadleafcommerce.core.order.domain.FulfillmentOption fulfillmentOption</query>
    </named-query>
    
    
    <named-query name="BC_READ_ALL_FULFILLMENT_OPTIONS_BY_TYPE" >
        <query>SELECT fulfillmentOption FROM org.broadleafcommerce.core.order.domain.FulfillmentOption fulfillmentOption 
        WHERE fulfillmentOption.fulfillmentType = :fulfillmentType</query>
    </named-query>
    
    

</entity-mappings>