<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">
    <description>Data Layer Extensibility Demo</description>
    <package>com.springcommerce.demo.user.domain.map</package>
    <entity class="com.springcommerce.demo.user.domain.ProprietaryCatalog" name="ProprietaryCatalog">
	    <table name="ProprietaryCatalog"/>
	    <attribute-override name="itemNumber">
	    	<column name="sku"/>
	    </attribute-override>
	    <attribute-override name="color">
	    	<column name="item_hue"/>
	    </attribute-override>
	    <attribute-override name="style">
	    	<column name="item_style"/>
	    </attribute-override>
        <attributes>
            <basic name="popularity">
                <column name="item_popularity"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>