<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<defaultCache
        maxElementsInMemory="100000"
        eternal="false"
        overflowToDisk="true"
        timeToLiveSeconds="1"
    	timeToIdleSeconds="1">
    	<cacheEventListenerFactory class="org.broadleafcommerce.profile.cache.HydratedCacheEventListenerFactory"/>
   	</defaultCache>
   	
	<cache
    	name="org.hibernate.cache.StandardQueryCache"
    	maxElementsInMemory="100000"
    	eternal="false"
    	overflowToDisk="true"
    	timeToLiveSeconds="1"
    	timeToIdleSeconds="1">
    </cache>
    	
   	<cache
    	name="blOrderElements"
    	maxElementsInMemory="1000000"
    	eternal="false"
    	overflowToDisk="true"
    	timeToLiveSeconds="1"
    	timeToIdleSeconds="1"/>
    	
</ehcache>
