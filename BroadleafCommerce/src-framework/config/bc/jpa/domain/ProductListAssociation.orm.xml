<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" version="1.0">

	<entity class="org.broadleafcommerce.catalog.domain.ProductListAssociation" name="productListAssociation">
        <table name="BLC_PRODUCT_LIST_ASSOCIATION" >        	
        </table>        
        <inheritance strategy="JOINED"/>

        <attributes>
            <id name="id">            	
            	<column name="ASSOCIATION_ID"/>
                <generated-value/>
                
            </id>
			
			<basic name="rank">
				<column name="RANK"/>
			</basic>

			<many-to-one name="product">
				<join-column name="PRODUCT_ID"/>
			</many-to-one>
			
			<many-to-one name="productList">
				<join-column name="PRODUCT_LIST_ID"/>
			</many-to-one>
        </attributes>
    </entity>


</entity-mappings>