<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" version="1.0">

	<entity class="org.broadleafcommerce.catalog.domain.BroadleafProductList" name="productList">
        <table name="BLC_PRODUCT_LIST" >
        </table>
        <inheritance strategy="JOINED"/>
		<discriminator-column name="type"/>
        <attributes>
            <id name="id">
            	<column name="PRODUCT_LIST_ID"/>
                <generated-value/>
            </id>
            <basic name="name">
                <column name="NAME"/>
            </basic>
            <basic name="dynamicFlag">
                <column name="DYNAMIC_FLAG"/>
            </basic>
            <basic name="dynamicQuery">
                <column name="DYNAMIC_QUERY"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>