<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" version="1.0">

	<named-query name="READ_SHOPPING_CART_PROMOTION_BY_ID" >
		<query>
			SELECT shoppingCartPromotion FROM org.broadleafcommerce.rules.domain.ShoppingCartPromotion shoppingCartPromotion
			WHERE shoppingCartPromotion.id = :id
		</query>
	</named-query>

	<named-query name="READ_ALL_SHOPPING_CART_PROMOTIONS" >
		<query>
			SELECT shoppingCartPromotion FROM org.broadleafcommerce.rules.domain.ShoppingCartPromotion shoppingCartPromotion
		</query>
	</named-query>

</entity-mappings>