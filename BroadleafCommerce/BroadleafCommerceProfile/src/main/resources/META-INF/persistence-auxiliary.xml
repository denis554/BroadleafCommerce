<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">
	<persistence-unit name="blPU" transaction-type="RESOURCE_LOCAL">
		<class>org.broadleafcommerce.email.domain.EmailTrackingClicksImpl</class>
		<class>org.broadleafcommerce.email.domain.EmailTrackingImpl</class>
		<class>org.broadleafcommerce.email.domain.EmailTrackingOpensImpl</class>
		<exclude-unlisted-classes/>
		<properties>
			<property name="hibernate.archive.autodetection" value="false" />
        </properties>
	</persistence-unit>
</persistence>